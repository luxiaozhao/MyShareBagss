<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF"
    android:orientation="vertical"
    tools:context="administrator.example.com.baobao.ui.user.SetActivity">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200px">


        <ImageView
            android:id="@+id/set_return"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="60px"
            android:src="@mipmap/xiaoyu3" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/set_set"
            android:textSize="70px" />
        <!--android:textStyle="bold"-->



    </RelativeLayout>
<View
    android:layout_width="match_parent"
    android:layout_height="5px"
    android:background="#E3E3E3"/>





    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/set_safety1"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50px"
                android:text="@string/set_safety" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/textView7"
                android:layout_marginEnd="36dp"
                android:src="@mipmap/wode_zhankaiyou3" />


        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5px"
            android:background="#E3E3E3"
            android:layout_marginLeft="50px"
            android:layout_marginRight="50px"/>

        <RelativeLayout
            android:id="@+id/set_address1"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:gravity="center_vertical">


            <TextView
                android:id="@+id/text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50px"
                android:text="@string/set_address" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/text2"
                android:layout_marginEnd="36dp"
                android:src="@mipmap/wode_zhankaiyou3" />


        </RelativeLayout>

        <!--<string name="button_dropout" >退出登录</string>-->

        <View
            android:layout_width="match_parent"
            android:layout_height="5px"
            android:background="#E3E3E3"
            android:layout_marginLeft="50px"
            android:layout_marginRight="50px"/>

        <RelativeLayout
            android:id="@+id/set_updated1"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:gravity="center_vertical">


            <TextView
                android:id="@+id/text3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50px"
                android:text="@string/set_updated" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/text3"
                android:layout_marginEnd="36dp"
                android:src="@mipmap/wode_zhankaiyou3" />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="5px"
            android:background="#E3E3E3"
            android:layout_marginLeft="50px"
            android:layout_marginRight="50px"/>

        <RelativeLayout
            android:id="@+id/set_clear1"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:gravity="center_vertical">


            <TextView
                android:id="@+id/text4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50px"
                android:text="@string/set_clear" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/text4"
                android:layout_marginEnd="36dp"
                android:src="@mipmap/wode_zhankaiyou3" />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="5px"
            android:background="#E3E3E3"
            android:layout_marginLeft="50px"
            android:layout_marginRight="50px"/>
        <RelativeLayout
            android:id="@+id/set_on1"
            android:layout_width="match_parent"
            android:layout_height="150px"
            android:gravity="center_vertical">


            <TextView
                android:id="@+id/text5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50px"
                android:text="@string/set_on" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/text5"
                android:layout_marginEnd="36dp"
                android:src="@mipmap/wode_zhankaiyou3" />


        </RelativeLayout>



    </LinearLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <Button
            android:id="@+id/set_dropout"
            android:layout_marginLeft="200px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FC655E"
            android:text="@string/button_dropout"
            android:textColor="#FFF"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="200px" />

    </RelativeLayout>

</LinearLayout>